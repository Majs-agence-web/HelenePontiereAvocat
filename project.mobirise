          "_customHTML": "<section class=\"carousel slide testimonials-slider testimonials3 modernm4_testimonials3\" group=\"Testimonials\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"BootstrapCarouselSwipe,mbr-testimonials-slider\" data-interval=\"false\">\n\n  <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"range\" title=\"Padding Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" inline>\n    <input type=\"range\" title=\"Padding Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\" inline>\n    <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n    <header>Carousel</header>\n    <input type=\"color\" title=\"Card Color\" value=\"#ffffff\" name=\"cardColor\">\n    <select name=\"columns\" title=\"Show Card\">\n      <option value=\"1\">1</option>\n      <option value=\"2\" selected>2</option>\n      <option value=\"3\">3</option>\n      <option value=\"4\">4</option>\n    </select>\n    <select name=\"cardsAmount\" title=\"Cards Amount\">\n      <option value=\"1\" selected>1</option>\n      <option value=\"2\">2</option>\n    </select>\n    <input type=\"checkbox\" title=\"Autoplay\" name=\"showAutoplay\">\n    <input type=\"range\" inline title=\"Autoplay Interval (ms)\" name=\"interval\" min=\"1000\" max=\"8000\" step=\"1000\" value=\"5000\" condition=\"showAutoplay\">\n    <input type=\"color\" title=\"Color Indicators\" name=\"colorCircle\" value=\"#efefef\">\n    <input type=\"checkbox\" title=\"Show Author\" name=\"showAuthor\" checked>\n    <header>Background</header>\n    <fieldset type=\"background\" name=\"bg\" parallax>\n      <input type=\"image\" title=\"Image\" value>\n      <input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n      <input type=\"color\" title=\"Color\" value=\"#efefef\" selected>\n   